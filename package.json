{
  "name": "capacitor-braintree-dropin",
  "version": "0.1.0",
  "description": "A Capacitor plugin for the Braintree mobile payment processing SDK (forked from https://github.com/sumbria/capacitor-braintree)",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "watch": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "author": "Alberto Starosta <albertix.com@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@capacitor/core": "latest"
  },
  "devDependencies": {
    "typescript": "^3.2.4",
    "@capacitor/ios": "latest",
    "@capacitor/android": "latest"
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorBraintree.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native",
    "braintree",
    "dropIn"
  ],
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/albertixcom/capacitor-braintree-dropin"
  },
  "bugs": {
    "url": "https://github.com/albertixcom/capacitor-braintree-dropin/issues"
  }
}
